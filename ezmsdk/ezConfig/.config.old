CONFIG_APP_VERSION_NUMBER="00.00.00"
CONFIG_SDK_VERSION_NUMBER="00.00.99"
# CONFIG_NO_CHIP is not set
CONFIG_WIN=y
# CONFIG_LINUX is not set
# CONFIG_ESP32 is not set
# CONFIG_STM32 is not set
CONFIG_UNITY_UNIT_TEST=y

#
# EMBEDDED_EMULATOR support disable. Depend on LOGBIN_PARSER and EZ_QUEUE
#

#
# Helper modules
#
CONFIG_HELPER_LINKEDLIST=y

#
# LOGGING configuration
#
CONFIG_LOGGING=y
CONFIG_USING_COLOR=y
# end of LOGGING configuration

CONFIG_HELPER_HEXDUMP=y
CONFIG_RING_BUFFER=y
CONFIG_HELPER_ASSERT=y
CONFIG_EVENT_NOTIFIER=y

#
# STCMEM configuration
#
CONFIG_STCMEM=y
CONFIG_NUM_OF_MEM_BLOCK=128
# end of STCMEM configuration

CONFIG_SYSTEM_ERROR=y

#
# EZ_QUEUE configuration
#
CONFIG_EZ_QUEUE=y
# CONFIG_EZ_QUEUE_TEST is not set
# end of EZ_QUEUE configuration
# end of Helper modules

#
# Driver modules
#

#
# HAL_UART configuration
#
CONFIG_HAL_UART=y
CONFIG_SIM_UART=y
CONFIG_VIRTUAL_COM=y
# end of HAL_UART configuration

#
# FLASH_SIM configuration
#
# CONFIG_FLASH_SIM is not set
# end of FLASH_SIM configuration

# CONFIG_WIFI_CONTROLLER is not set
# CONFIG_MQTT is not set
# end of Driver modules

#
# Application modules
#

#
# CLI configuration
#
CONFIG_CLI=y
CONFIG_NUM_OF_ARGUMENT=4
CONFIG_NUM_OF_CMD=10
# end of CLI configuration

# CONFIG_SCHEDULER is not set
# CONFIG_SMALLOC is not set

#
# BIN_PARSER configuration
#
# CONFIG_BIN_PARSER is not set
# end of BIN_PARSER configuration

#
# RPC configuration
#
CONFIG_RPC=y
CONFIG_NUM_OF_REQUEST=10
CONFIG_EZ_RPC_TEST=y
# end of RPC configuration

#
# IPC configuration
#
CONFIG_IPC=y
CONFIG_NUM_OF_IPC_INSTANCE=5
CONFIG_USING_MODULE_NAME=y
# end of IPC configuration

#
# KERNEL configuration
#
CONFIG_KERNEL=y
CONFIG_NUM_OF_PROCESS=5
CONFIG_NUM_OF_KERNEL_TASK=10
CONFIG_KERNEL_TEST=y
# end of KERNEL configuration

#
# DRIVERINF configuration
#
CONFIG_DRIVERINF=y
CONFIG_DRIVERINF_TEST=y
# end of DRIVERINF configuration

CONFIG_DATA_MODEL=y
CONFIG_STATEMACHINE=y
# end of Application modules
